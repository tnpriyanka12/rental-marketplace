<%= form_for @property do |f| %>

<fieldset>
  <%= f.label :username %>
  <%= f.text_field :username %>
</fieldset>

<fieldset>
  <%= f.label :full_name  %>
  <%= f.text_field :full_name %>
</fieldset>

<fieldset>
  <%= f.label :profile_pic %>
  <%= f.text_field :profile_pic %>
</fieldset>

<fieldset>
  <%= f.label :description %>
  <%= f.text_field :description %>
</fieldset>

<%= f.submit %>


<% end %>
